<Application x:Class="KeyphraseExtraction.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:KeyphraseExtraction.BaseClass"
             StartupUri="View\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <!-- Set default skin -->
                <ResourceDictionary Source="Themes\Skins\ExpressionDark.xaml"/>
                <ResourceDictionary Source="Themes\WindowStyle.xaml"/>

            </ResourceDictionary.MergedDictionaries>

            <local:SettingConverter x:Key="SettingConv" />

        </ResourceDictionary>
    </Application.Resources>
</Application>
